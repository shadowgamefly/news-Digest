tleSynthesized": true, "detectedLanguage": "en", "license": 0, "id": "3791eee84662", "createdAt": 1492022895454, "inResponseToRemovedAt": 0, "isRequestToPubDisabled": false, "responseHiddenOnParentPostAt": 0, "newsletterId": "", "title": "I think chatting with a native is the best advice you can give to a language learner.", "slug": "i-think-chatting-with-a-native-is-the-best-advice-you-can-give-to-a-language-learner", "versionId": "c4302072aa03", "inResponseToMediaResourceId": "a14b450a3395c33d62d0d2c525c579d4", "isSubscriptionLocked": false, "updatedAt": 1492023494484, "mediumUrl": "", "previewContent": {"bodyModel": {"sections": [{"startIndex": 0}], "paragraphs": [{"text": "I think chatting with a native is the best advice you can give to a language learner. I had to take a test in German as a foreign language (TestDaF) and couldn\u2019t afford any preparation classes or books. So I spoke to German people and I believe that helped me achieve maximum points possible on this test.", "name": "d30d", "alignment": 1, "markups": [], "type": 1}]}, "isFullContent": false}, "webCanonicalUrl": "", "notifyFacebook": false, "experimentalCss": "", "virtuals": {"links": {"entries": [], "generatedAt": 1492023495253, "version": "0.3"}, "imageCount": 0, "socialRecommendsCount": 0, "wordCount": 113, "totalClapCount": 0, "responsesCreatedCount": 0, "isLockedPreviewOnly": false, "usersBySocialRecommends": [], "isBookmarked": false, "recommends": 0, "subtitle": "Apps, on the other hand, don\u2019t help me much. I installed Duolingo to practice my Norwegian but I keep forgetting the words it throws at me\u2026", "takeoverId": "", "metaDescription": "", "previewImage": {"originalHeight": 0, "originalWidth": 0, "imageId": "", "height": 0, "filter": "", "width": 0, "strategy": "resample", "backgroundSize": ""}, "readingTime": 0.42641509433962266, "allowNotes": true, "tags": []}, "allowResponses": true, "homeCollectionId": "", "isSponsored": false, "importedUrl": "", "vote": false, "accep