blishedVersion": "9e3790d4d917", "hasUnpublishedEdits": false, "notifyTwitter": false, "isApprovedTranslation": false, "creatorId": "32b605cf84cd", "visibility": 0, "translationSourcePostId": "", "latestPublishedAt": 1477958012525, "canonicalUrl": "", "coverless": true, "migrationId": "", "uniqueSlug": "great-story-ron-5e3aa35d2faf", "latestVersion": "9e3790d4d917", "isSeries": false}, "f71f22a936cb": {"importedPublishedAt": 0, "inResponseToPostId": "1e2e304c07f1", "notifyFollowers": true, "seriesLastAppendedAt": 0, "translationSourceCreatorId": "", "displayAuthor": "", "isTitleSynthesized": true, "detectedLanguage": "en", "license": 0, "id": "f71f22a936cb", "createdAt": 1477859033989, "inResponseToRemovedAt": 0, "isRequestToPubDisabled": false, "responseHiddenOnParentPostAt": 0, "newsletterId": "", "title": "How gracious of you, and I\u2019ve been rough with you, I know.", "slug": "how-gracious-of-you-and-ive-been-rough-with-you-i-know", "versionId": "9b38caed2c13", "inResponseToMediaResourceId": "9607e59d063493487bc0a92f644e24ae", "isSubscriptionLocked": false, "updatedAt": 1477859515213, "mediumUrl": "", "previewContent": {"bodyModel": {"sections": [{"startIndex": 0}], "paragraphs": [{"text": "How gracious of you, and I\u2019ve been rough with you, I know. It is a man\u2019s roughness given a worthy adversary: I have not spared you in disagreement, because I think highly enough of you not to think I need to. This game between us, is not rigged. Neither player, has privilege.", "name": "a658", "alignment": 1, "markups": [], "type": 1}]}, "isFullContent": false}, "webCanonicalUrl": "", "notifyFacebook": false, "experimentalCss": "", "virtuals": {"links": {"entries": [{"alts": [{"url": "medium://p/86be7804ea67", "type": 2}, {"url": "medium://p/86be7804ea67", "type": 3}], "url": "https://medium.com/@framersqool/ill-take-the-liberty-of-commenting-on-my-own-comments-to-add-more-anecdotal-entirely-86be7804ea67#.40io4djau"}], "generatedAt": 1477859515449, "version": "0.3