": 0}, "d2d35c6ec2fd": {"latestPublishedAt": 1492192400195, "notifyTwitter": false, "displayAuthor": "", "responseHiddenOnParentPostAt": 0, "uniqueSlug": "i-feel-like-the-first-round-matchup-between-harden-and-westbrook-will-have-a-dramatic-impact-on-how-d2d35c6ec2fd", "isSubscriptionLocked": false, "importedUrl": "", "migrationId": "", "virtuals": {"isBookmarked": false, "socialRecommendsCount": 0, "allowNotes": true, "readingTime": 0.2981132075471698, "previewImage": {"originalWidth": 0, "originalHeight": 0, "width": 0, "backgroundSize": "", "imageId": "", "height": 0, "filter": "", "strategy": "resample"}, "metaDescription": "", "imageCount": 0, "subtitle": "", "links": {"version": "0.3", "entries": [], "generatedAt": 1492192400511}, "usersBySocialRecommends": [], "responsesCreatedCount": 1, "takeoverId": "", "wordCount": 79, "totalClapCount": 0, "isLockedPreviewOnly": false, "recommends": 0, "tags": []}, "vote": false, "experimentalCss": "", "license": 0, "translationSourcePostId": "", "isSeries": false, "slug": "i-feel-like-the-first-round-matchup-between-harden-and-westbrook-will-have-a-dramatic-impact-on-how", "createdAt": 1492192153585, "latestVersion": "26a01633cbd7", "mediumUrl": "", "type": "Post", "latestRev": 43, "seriesLastAppendedAt": 0, "coverless": true, "translationSourceCreatorId": "", "creatorId": "4e15dde2f1f2", "latestPublishedVersion": "26a01633cbd7", "allowResponses": true, "webCanonicalUrl": "", "versionId": "26a01633cbd7", "title": "I feel like the first-round matchup between Harden and Westbrook will have a dramatic impact on how\u2026", "inResponseToRemovedAt": 0, "inResponseToPostId": "f78effb25114", "canonicalUrl": "", "isSponsored": false, "isApprovedTranslation": false, "firstPublishedAt": 1492192400195, "content": {"postDisplay": {"coverless": true}}, "isTitleSynthesized": true, "importedPublishedAt": 0, "approvedHomeCollectionId": "", "newsletterId": "", "notifyFacebook": false, "hasUnpublishedEdits": false, "inResponseToMediaResour